pipeline{
    agent any

    stages{
        stage('cloning_git_repo'){
            steps{
                git 'https://github.com/zakics444/simple-java-maven-app.git'
            }
        }
        stage('building'){
            steps{
                sh 'mvn clean package'
            }
        }
        stage('release'){
            steps{
            archive 'target/*.jar'
            }
        }
    }
}
